<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#fdff72" />
      <stroke
          android:width="1dp"
          android:color="#fdff72" />
      <corners
          android:radius="4dp" />

    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#fdff72"
          android:endColor="#ffe63b"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#fdff72" />
      <corners
          android:radius="4dp" />
    </shape>
  </item>
</selector>
